end)

tab:CreateSlider("JumpPower",50,250,function(a)
    Players.LocalPlayer.Character.Humanoid.JumpPower = a
end)

tab:CreateSlider("FOV",70,250,function(a)
    game.Workspace.CurrentCamera.FieldOfView = a
end)

